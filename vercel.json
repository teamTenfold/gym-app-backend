{
  "version": 2,

  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js",
      "headers": {
        "Connection": "Upgrade",
        "Upgrade": "websocket"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 3008,
      "maxDuration": 300
    }
  }
}
